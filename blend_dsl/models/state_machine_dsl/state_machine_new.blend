{
	"nodeTypes": {
	  "events": {
		"interfaceConfig": {
		  "title": "Events",
		  "subtitle": "Defines an events",
		  "icon": "EmojiEventsIcon",
		  "draggable": true,
		  "handles": [
			{
			  "type": "target"
			}
		  ],
		  "parameters": [
			{ "name": "events", "inputType": "dynamicInputs"}
		  ]
		},
		"grammar": {
		  "endKeyword": "end",
		  "startPattern": "events",
		  "maxInstances": 1,
		  "parameters": [],
		  "nestedParameters": [
			  {
				"name": "events",
				"parameterStart": null,
				"prefix": "",
				"delimiter": "",
				"parameters": ["name", "code"],
				"unQuoted": true
			  }
			]
		}
	  },
	  "commands": {
		"interfaceConfig": {
		  "title": "Commands",
		  "subtitle": "Defines a commands",
		  "icon": "CodeIcon",
		  "draggable": true,
		  "handles": [
			{
			  "type": "target"
			}
		  ],
		  "parameters": [
			{ "name": "commands", "inputType": "dynamicInputs"}
		  ]
		},
		"grammar": {
		  "endKeyword": "end",
		  "startPattern": "commands",
		  "maxInstances": 1,
		  "parameters": [],
		  "nestedParameters": [
			  {
				"name": "commands",
				"parameterStart": null,
				"prefix": "",
				"delimiter": "",
				"parameters": ["name", "type"],
				"unQuoted": true
			  }
			]
		  
		}
	  },
	  "resetevents": {
		"interfaceConfig": {
		  "title": "resetEvents",
		  "subtitle": "Defines a Reset Events",
		  "icon": "RestartAltIcon",
		  "draggable": true,
		  "handles": [
			{
			  "type": "source",
			  "parameterDependency": "resetEvents",
			  "handleTarget": {
				"nodeType": "events",
				"parameter": "events.name"
			  },
			  "connectionLimit": {
				"events": "-1",
				"resetevents": "-1"
			  }
			}
		  ],
		  "colorConfig": {
			"resetEvents": {
			  "startColor": "#E0BE19",
			  "endColor": "#615A3B"
			}
		  },
		  "parameters": [
			{ "name": "resetEvents", "inputType": "autocomplete", "multiple": true}
		  ]
		},
		"grammar": {
		  "endKeyword": "end",
		  "startPattern": "resetEvents",
		  "maxInstances": 1,
		  "parameters": [],
		  "nestedParameters": [
			  {
				"label": "Reset Events",
				"name": "resetEvents",
				"parameterStart": null,
				"prefix": "",
				"options": "events"
			  }
			]
		  ,
		  "dependencies": {
			"events": {
			  "format": "${events.name}"
			}
		  }
		}
	  },
	  "state": {
		"interfaceConfig": {
		  "title": "State",
		  "subtitle": "Defines a state",
		  "icon": "ControlCameraIcon",
		  "draggable": true,
		  "handles": [
			{
			  "type": "source",
			  "style": {
				"top": 20,
				"background": "#00FF00"
			  },
			  "parameterDependency": "transitions.event",
			  "handleTarget": {
				"nodeType": "events",
				"parameter": "events.name"
			  },
			  "connectionLimit": {
				"state": "-1",
				"events": "-1"
			  }
			},
			{
			  "type": "source",
			  "style": {
				"top": 40,
				"background": "#00FF00"
			  },
			  "parameterDependency": "actions",
			  "handleTarget": {
				"nodeType": "commands",
				"parameter": "commands.name"
			  },
			  "connectionLimit": {
				"state": "-1",
				"commands": "-1"
			  }
			},
			{
			  "type": "source",
			  "style": {
				"top": 60,
				"background": "#00FF00"
			  },
			  "parameterDependency": "transitions.state",
			  "handleTarget": "state",
			  "connectionLimit": {
				"state": "-1"
			  }
			},
			{
			  "type": "target"
			}
		  ],
		  "colorConfig": {
			"transitions.event": {
			  "startColor": "#E0BE19",
			  "endColor": "#FFD91C"
			},
			"actions": {
			  "startColor": "#E06C27",
			  "endColor": "#FF6830"
			},
			"transitions.state": {
			  "startColor": "#19E0D9",
			  "endColor": "#1CFFF7"
			}
		  },
		  "parameters": [
			{
				"name": "title",
				"default": "random"
			  },{
				"name": "actions",
				"inputType": "autocomplete",
				"multiple": true
			  },
			{ "name": "transitions", 
			"inputType": "dynamicInputs",
			"inputTypes": ["dropdown", "dropdown"]}
		  ]
		},
		"grammar": {
		  "endKeyword": "end",
		  "startPatternRegex": "state (\\w+)",
		  "startPattern": "state ${title}",
		  "parameters": [
			{
			  "name": "title"
			},
			{
			  "name": "actions",
			  "prefix": "actions",
			  "type": "array",
			  "complexDependency": ["commands"],
			  "startDelimiter": "{",
			  "optionsDelimiter": " ",
			  "endDelimiter": "}",
			  "options": ["commands"]
			}
		  ],
		  "nestedParameters": [
			  {
				"name": "transitions",
				"prefix": "",
				"parameterStart": null,
				"delimiter": "=>",
				"parameters": ["event", "state"],
				"options": ["events", "state"]
			  }
			],
		  "dependencies": {
			"events": {
			  "format": "${events.name}"
			},
			"commands": {
			  "format": "${commands.name}"
			},
			"state": {
			  "parameter": "title"
			}
		  }
		}
	  }
	}
  }
  