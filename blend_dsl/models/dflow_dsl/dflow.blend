			},
			"dependencies": {
				"events": {
					"format": "${events.name}"
				}
			}
		},
		"state": {
			"title": "State",
			"subtitle": "Defines a state",
			"icon": "ControlCameraIcon",
			"draggable": true,
			"handles": [
				{
					"type": "source",
					"style": { "top": 20, "background": "#00FF00" },
