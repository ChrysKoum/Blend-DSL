					"handleTarget": {"nodeType": "events", "parameter": "events.name"},
					"connectionLimit": {"state": "-1", "events": "-1"}
				},
				{
					"type": "source",
					"style": { "top": 40, "background": "#00FF00" },
					"parameterDependency": "actions",
					"handleTarget": {"nodeType": "commands", "parameter": "commands.name"},
					"connectionLimit": {"state": "-1", "commands": "-1"}
				},
				{
					"type": "source",
					"style": { "top": 60, "background": "#00FF00" },
					"parameterDependency": "transitions.state",
					"handleTarget": "state",
					"connectionLimit": {"state": "-1"}
				},
				{ "type": "target" }
			],
			"colorConfig": {
				"transitions.event": { "startColor": "#E0BE19", "endColor": "#FFD91C" },
				"actions": { "startColor": "#E06C27", "endColor": "#FF6830" },
				"transitions.state": { "startColor": "#19E0D9", "endColor": "#1CFFF7" }
			},
			"endKeyword": "end",
			"startPattern": "state ${title}",
			"parameters": [
				{
					"name": "title",
					"default": "random"
				},
				{
					"name": "actions",
					"prefix": "actions",
					"type": "array",
					"inputType": "autocomplete",
					"startDelimiter": "{",
					"optionsDelimiter": " ",
					"endDelimiter": "}",
					"options": ["commands"],
					"complexDependency": ["commands"],
					"multiple": true
				}
			],
			"nestedParameters": {
				"transitions": [
					{
						"name": "transitions",
						"prefix": "",
						"parameterStart": null,
						"delimiter": "=>",
						"inputType": "dynamicInputs",
						"parameters": ["event", "state"],
						"options": ["events", "state"],
						"inputTypes": ["dropdown", "dropdown"]
					}
				]
			},
			"dependencies": {
				"events": {
					"format": "${events.name}"
				},
				"commands": {
					"format": "${commands.name}"
				},
				"state": {
					"parameter":"title"
				}
			}
		}
